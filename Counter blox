end

-- âœ… MODIFIED HUB CREATION TO INCLUDE NEW SECTIONS
-- In the createHubInterface function, after existing sections, add:
function createEnhancedHub()
    createHubInterface() -- Call original function
    
    -- Wait for original UI to create
    wait(0.1)
    
    -- Find the scrolling frame and add enhanced sections
    local scrollingFrame = MAIN_FRAME:FindFirstChild("Content"):FindFirstChild("ScrollingFrame")
    if scrollingFrame then
        createEnhancedSections(scrollingFrame)
        
        -- Update canvas size for new content
        scrollingFrame.CanvasSize = UDim2.new(0, 0, 0, 1000)
    end
end

-- âœ… ENHANCED MAIN LOOP
RunService.RenderStepped:Connect(function()
    -- Original functionality
    if CuTins.AIMBOT.ENABLED and TARGET then
        aimAtTarget()
    end
    
    -- NEW: Enhanced features
    handleAutoFire()
    
    -- NEW: Anti-Aim spin
    if CuTins.MOVEMENT.ANTI_AIM then
        ANTI_AIM_ANGLE = (ANTI_AIM_ANGLE + CuTins.MOVEMENT.SPIN_SPEED) % 360
        local character = LocalPlayer.Character
        if character then
            local rootPart = character:FindFirstChild("HumanoidRootPart")
            if rootPart then
                rootPart.CFrame = rootPart.CFrame * CFrame.Angles(0, math.rad(ANTI_AIM_ANGLE), 0)
            end
        end
    end
    
    -- NEW: Update tracers
    if CuTins.ESP.TRACERS then
        updateTracersPosition()
    end
    
    -- ESP handling
    if CuTins.ESP.ENABLED then
        for _, player in pairs(Players:GetPlayers()) do
            if player ~= LocalPlayer and player.Character and not ESP_ITEMS[player] then
                createESP(player)
            end
        end
    end
end)

-- âœ… NEW: Update tracer positions
function updateTracersPosition()
    for player, items in pairs(ESP_ITEMS) do
        if items.Tracer and player.Character then
            local rootPart = player.Character:FindFirstChild("HumanoidRootPart")
            if rootPart then
                local screenPos, onScreen = Camera:WorldToViewportPoint(rootPart.Position)
                if onScreen then
                    items.Tracer.Visible = true
                    items.Tracer.Position = UDim2.new(0, screenPos.X, 0, screenPos.Y)
                    
                    -- Calculate angle and length
                    local center = Vector2.new(Camera.ViewportSize.X/2, Camera.ViewportSize.Y/2)
                    local target = Vector2.new(screenPos.X, screenPos.Y)
                    local direction = (target - center).Unit
                    
                    items.Tracer.Size = UDim2.new(0, (target - center).Magnitude, 0, 1)
                    items.Tracer.Rotation = math.deg(math.atan2(direction.Y, direction.X))
                else
                    items.Tracer.Visible = false
                end
            end
        end
    end
end

-- âœ… ENHANCED INPUT HANDLING
UserInputService.InputBegan:Connect(function(input, processed)
    if processed then return end
    
    -- Original inputs
    if input.KeyCode == Enum.KeyCode.Insert then
        CuTins.UI_OPEN = not CuTins.UI_OPEN
        SCREEN_GUI.Enabled = CuTins.UI_OPEN
    end
    
    if input.UserInputType == CuTins.AIMBOT.MOUSE_BUTTON and CuTins.AIMBOT.ENABLED then
        TARGET = getClosestPlayer()
        if CuTins.AIMBOT.SILENT_AIM then
            TARGET = getSilentAimTarget() or TARGET
        end
    end
    
    if input.KeyCode == Enum.KeyCode.F then
        CuTins.FLY.ENABLED = not CuTins.FLY.ENABLED
        toggleFly()
        updateStatus()
    end
    
    -- NEW: Quick toggle for speed hack
    if input.KeyCode == Enum.KeyCode.V then
        CuTins.MOVEMENT.SPEED = not CuTins.MOVEMENT.SPEED
        updateSpeed()
        updateStatus()
    end
    
    -- NEW: Quick toggle for no clip
    if input.KeyCode == Enum.KeyCode.N then
        CuTins.FLY.NOCLIP = not CuTins.FLY.NOCLIP
        toggleNoClip()
        updateStatus()
    end
end)

-- âœ… ENHANCED INITIALIZATION
createEnhancedHub() -- Use enhanced hub instead of original
createWatermark()
updateStatus()

-- Enhanced player tracking with new features
for _, player in pairs(Players:GetPlayers()) do
    if player ~= LocalPlayer and CuTins.ESP.ENABLED and player.Character then
        createESP(player)
    end
    player.CharacterAdded:Connect(function(char)
        wait(1)
        if CuTins.ESP.ENABLED then
            createESP(player)
        end
    end)
end

