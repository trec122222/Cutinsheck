    ESP = {
        ENABLED = false,
        TEAM_CHECK = true,
        BOX = true,
        NAMES = true,
        HEALTHBAR = true,
        DISTANCE = true,
        WEAPON = true
    },
    
    HITBOX = {
        ENABLED = false,
        SIZE = 2.0
    },
    
    -- WEAPON
    WEAPON = {
        NO_RECOIL = false,
        NO_SPREAD = false,
        RAPID_FIRE = false,
        INSTANT_RELOAD = false,
        INFINITE_AMMO = false,
        DAMAGE_MULTIPLIER = 1.0
    },
    
    -- MOVEMENT
    MOVEMENT = {
        FLY = false,
        FLY_SPEED = 3,
        SPEED = false,
        SPEED_VALUE = 30,
        JUMP = false,
        JUMP_VALUE = 75,
