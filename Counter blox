        INSTANT_RESPAWN = false
    }
}

-- VARIABLES
local TARGET = nil
local ESP_ITEMS = {}
local BODYGYRO, BODYVELOCITY = nil, nil
local NOCLIP_CONNECTION = nil
local BUNNYHOP_CONNECTION = nil
local CROSSHAIR_GUI = nil

-- ‚úÖ ENHANCED UI CREATION
local function createEnhancedUI()
    local SCREEN_GUI = Instance.new("ScreenGui")
    SCREEN_GUI.Name = "CuTinsFPSUtils"
    SCREEN_GUI.Parent = game:GetService("CoreGui")

    -- Main Frame
    local MAIN_FRAME = Instance.new("Frame")
    MAIN_FRAME.Size = UDim2.new(0, 350, 0, 600)
    MAIN_FRAME.Position = UDim2.new(0, 20, 0.5, -300)
    MAIN_FRAME.BackgroundColor3 = Color3.fromRGB(20, 20, 30)
    MAIN_FRAME.BorderSizePixel = 2
    MAIN_FRAME.BorderColor3 = Color3.fromRGB(255, 50, 50)
    MAIN_FRAME.Active = true
    MAIN_FRAME.Draggable = true
    MAIN_FRAME.Parent = SCREEN_GUI

    -- Title
    local Title = Instance.new("TextLabel")
    Title.Size = UDim2.new(1, 0, 0, 40)
    Title.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
    Title.Text = "CuTins FPS UTILITIES 2025"
    Title.TextColor3 = Color3.white
    Title.TextSize = 16
    Title.Font = Enum.Font.GothamBold
    Title.Parent = MAIN_FRAME

    -- Close Button
    local CloseBtn = Instance.new("TextButton")
    CloseBtn.Size = UDim2.new(0, 30, 0, 30)
    CloseBtn.Position = UDim2.new(1, -35, 0, 5)
    CloseBtn.BackgroundColor3 = Color3.fromRGB(200, 0, 0)
    CloseBtn.Text = "X"
    CloseBtn.TextColor3 = Color3.white
    CloseBtn.TextSize = 14
    CloseBtn.Font = Enum.Font.GothamBold
    CloseBtn.Parent = MAIN_FRAME

    -- Content
    local Content = Instance.new("ScrollingFrame")
    Content.Size = UDim2.new(1, -20, 1, -60)
    Content.Position = UDim2.new(0, 10, 0, 50)
    Content.BackgroundTransparency = 1
    Content.CanvasSize = UDim2.new(0, 0, 0, 1500)
    Content.ScrollBarThickness = 4
    Content.Parent = MAIN_FRAME

    -- Enhanced Toggle Function
    local function createToggle(name, category, setting, yPos)
        local btn = Instance.new("TextButton")
        btn.Size = UDim2.new(1, 0, 0, 35)
        btn.Position = UDim2.new(0, 0, 0, yPos)
        btn.BackgroundColor3 = CuTins[category][setting] and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(60, 60, 80)
        btn.Text = name .. ": " .. (CuTins[category][setting] and "ON" or "OFF")
        btn.TextColor3 = Color3.white
        btn.TextSize = 13
        btn.Font = Enum.Font.GothamBold
        btn.TextXAlignment = Enum.TextXAlignment.Left
        btn.Parent = Content
        
        btn.MouseButton1Click:Connect(function()
            CuTins[category][setting] = not CuTins[category][setting]
            btn.BackgroundColor3 = CuTins[category][setting] and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(60, 60, 80)
            btn.Text = name .. ": " .. (CuTins[category][setting] and "ON" or "OFF")
            
            -- Special handlers
            if category == "MOVEMENT" and setting == "NOCLIP" then
                toggleNoclip()
            elseif category == "MOVEMENT" and setting == "BUNNY_HOP" then
                toggleBunnyHop()
            elseif category == "VISUAL" and setting == "CROSSHAIR" then
                toggleCrosshair()
            elseif category == "VISUAL" then
                updateVisuals()
            end
        end)
        
        return btn
    end

    -- Create Category Sections
    local yPos = 0
    
    -- COMBAT SECTION
    createToggle("‚öîÔ∏è COMBAT FEATURES", "AIMBOT", "ENABLED", yPos).TextXAlignment = Enum.TextXAlignment.Center
    yPos = yPos + 40
    createToggle("üéØ Aimbot", "AIMBOT", "ENABLED", yPos)
    yPos = yPos + 35
    createToggle("üîµ Team Check", "AIMBOT", "TEAM_CHECK", yPos)
    yPos = yPos + 35
    createToggle("üëÅÔ∏è Visible Check", "AIMBOT", "VISIBLE_CHECK", yPos)
    yPos = yPos + 35
    createToggle("üî´ Triggerbot", "AIMBOT", "TRIGGERBOT", yPos)
    yPos = yPos + 35
    createToggle("üëª Silent Aim", "MISC", "SILENT_AIM", yPos)
    yPos = yPos + 35

    -- WEAPON SECTION
    createToggle("üî´ WEAPON MODS", "WEAPON", "NO_RECOIL", yPos).TextXAlignment = Enum.TextXAlignment.Center
    yPos = yPos + 40
    createToggle("üéØ No Recoil", "WEAPON", "NO_RECOIL", yPos)
    yPos = yPos + 35
    createToggle("üéØ No Spread", "WEAPON", "NO_SPREAD", yPos)
    yPos = yPos + 35
    createToggle("‚ö° Rapid Fire", "WEAPON", "RAPID_FIRE", yPos)
    yPos = yPos + 35
    createToggle("üîÅ Instant Reload", "WEAPON", "INSTANT_RELOAD", yPos)
    yPos = yPos + 35
    createToggle("‚àû Infinite Ammo", "WEAPON", "INFINITE_AMMO", yPos)
    yPos = yPos + 35

    -- MOVEMENT SECTION
    createToggle("üí® MOVEMENT", "MOVEMENT", "FLY", yPos).TextXAlignment = Enum.TextXAlignment.Center
    yPos = yPos + 40
    createToggle("üïäÔ∏è Fly Mode", "MOVEMENT", "FLY", yPos)
    yPos = yPos + 35
    createToggle("üèÉ Speed Hack", "MOVEMENT", "SPEED", yPos)
    yPos = yPos + 35
    createToggle("ü¶ò High Jump", "MOVEMENT", "JUMP", yPos)
    yPos = yPos + 35
    createToggle("üëª NoClip", "MOVEMENT", "NOCLIP", yPos)
    yPos = yPos + 35
    createToggle("üê∞ Bunny Hop", "MOVEMENT", "BUNNY_HOP", yPos)
    yPos = yPos + 35

    -- VISUAL SECTION
    createToggle("üëÅÔ∏è VISUAL MODS", "VISUAL", "BRIGHT_MAP", yPos).TextXAlignment = Enum.TextXAlignment.Center
    yPos = yPos + 40
    createToggle("üí° Bright Map", "VISUAL", "BRIGHT_MAP", yPos)
    yPos = yPos + 35
    createToggle("üåü FullBright", "VISUAL", "FULLBRIGHT", yPos)
    yPos = yPos + 35
    createToggle("üå´Ô∏è No Fog", "VISUAL", "NO_FOG", yPos)
    yPos = yPos + 35
    createToggle("üß± Wallhack", "VISUAL", "WALLHACK", yPos)
    yPos = yPos + 35
    createToggle("üéØ Crosshair", "VISUAL", "CROSSHAIR", yPos)
    yPos = yPos + 35

    -- MISC SECTION
    createToggle("‚öôÔ∏è MISC FEATURES", "MISC", "GOD_MODE", yPos).TextXAlignment = Enum.TextXAlignment.Center
    yPos = yPos + 40
    createToggle("üíÄ God Mode", "MISC", "GOD_MODE", yPos)
    yPos = yPos + 35
    createToggle("ü§ñ Auto Farm", "MISC", "AUTO_FARM", yPos)
    yPos = yPos + 35
    createToggle("üéØ Hitbox Expand", "HITBOX", "ENABLED", yPos)
    yPos = yPos + 35
    createToggle("üëÅÔ∏è ESP", "ESP", "ENABLED", yPos)
    yPos = yPos + 35

    -- Close Event
    CloseBtn.MouseButton1Click:Connect(function()
        SCREEN_GUI:Destroy()
    end)

    return SCREEN_GUI
end

-- ‚úÖ ENHANCED GAME FUNCTIONS

-- WEAPON MODIFICATIONS
local function weaponModifications()
    -- No Recoil & No Spread
    if CuTins.WEAPON.NO_RECOIL or CuTins.WEAPON.NO_SPREAD then
        local mt = getrawmetatable(game)
        local oldIndex = mt.__index
        
        setreadonly(mt, false)
        mt.__index = newcclosure(function(t, k)
            if CuTins.WEAPON.NO_RECOIL and string.lower(tostring(k)):find("recoil") then
                return 0
            end
            if CuTins.WEAPON.NO_SPREAD and string.lower(tostring(k)):find("spread") then
                return 0
            end
            return oldIndex(t, k)
        end)
        setreadonly(mt, true)
    end
end

-- MOVEMENT ENHANCEMENTS
local function toggleNoclip()
    if NOCLIP_CONNECTION then
        NOCLIP_CONNECTION:Disconnect()
        NOCLIP_CONNECTION = nil
    end
    
    if CuTins.MOVEMENT.NOCLIP then
        NOCLIP_CONNECTION = RunService.Stepped:Connect(function()
            if LocalPlayer.Character then
                for _, part in pairs(LocalPlayer.Character:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
        end)
    end
end

local function toggleBunnyHop()
    if BUNNYHOP_CONNECTION then
        BUNNYHOP_CONNECTION:Disconnect()
        BUNNYHOP_CONNECTION = nil
    end
    
    if CuTins.MOVEMENT.BUNNY_HOP then
        BUNNYHOP_CONNECTION = RunService.Heartbeat:Connect(function()
            if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
                LocalPlayer.Character.Humanoid.Jump = true
            end
        end)
    end
end

local function toggleFly()
    if BODYGYRO then BODYGYRO:Destroy() end
    if BODYVELOCITY then BODYVELOCITY:Destroy() end
    
    if not CuTins.MOVEMENT.FLY then return end
    
    local character = LocalPlayer.Character
    if not character then return end
    
    local rootPart = character:FindFirstChild("HumanoidRootPart")
    if not rootPart then return end
    
    BODYGYRO = Instance.new("BodyGyro")
    BODYGYRO.P = 1000
    BODYGYRO.D = 50
    BODYGYRO.MaxTorque = Vector3.new(4000, 4000, 4000)
    BODYGYRO.CFrame = rootPart.CFrame
    BODYGYRO.Parent = rootPart
    
    BODYVELOCITY = Instance.new("BodyVelocity")
    BODYVELOCITY.Velocity = Vector3.new(0, 0, 0)
    BODYVELOCITY.MaxForce = Vector3.new(4000, 4000, 4000)
    BODYVELOCITY.Parent = rootPart
end

-- VISUAL ENHANCEMENTS
local function updateVisuals()
    -- Bright Map
    if CuTins.VISUAL.BRIGHT_MAP then
        Lighting.Brightness = 5
        Lighting.Ambient = Color3.new(1, 1, 1)
    else
        Lighting.Brightness = 3
        Lighting.Ambient = Color3.new(0.5, 0.5, 0.5)
    end
    
    -- FullBright
    if CuTins.VISUAL.FULLBRIGHT then
        Lighting.GlobalShadows = false
    else
        Lighting.GlobalShadows = true
    end
    
    -- No Fog
    if CuTins.VISUAL.NO_FOG then
        Lighting.FogEnd = 100000
    else
        Lighting.FogEnd = 1000
    end
    
    -- Wallhack
    for _, part in pairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") then
            part.LocalTransparencyModifier = CuTins.VISUAL.WALLHACK and 0.5 or 0
        end
    end
end

local function toggleCrosshair()
    if CROSSHAIR_GUI then
        CROSSHAIR_GUI:Destroy()
        CROSSHAIR_GUI = nil
    end
    
    if CuTins.VISUAL.CROSSHAIR then
        CROSSHAIR_GUI = Instance.new("ScreenGui")
        CROSSHAIR_GUI.Name = "Crosshair"
        CROSSHAIR_GUI.Parent = game:GetService("CoreGui")
        
        local Center = Instance.new("Frame")
        Center.Size = UDim2.new(0, 4, 0, 4)
        Center.Position = UDim2.new(0.5, -2, 0.5, -2)
        Center.BackgroundColor3 = Color3.new(1, 0, 0)
        Center.BorderSizePixel = 0
        Center.Parent = CROSSHAIR_GUI
        
        local Top = Instance.new("Frame")
        Top.Size = UDim2.new(0, 2, 0, 15)
        Top.Position = UDim2.new(0.5, -1, 0.5, -20)
        Top.BackgroundColor3 = Color3.new(1, 1, 1)
        Top.BorderSizePixel = 0
        Top.Parent = CROSSHAIR_GUI
        
        local Bottom = Instance.new("Frame")
        Bottom.Size = UDim2.new(0, 2, 0, 15)
        Bottom.Position = UDim2.new(0.5, -1, 0.5, 5)
        Bottom.BackgroundColor3 = Color3.new(1, 1, 1)
        Bottom.BorderSizePixel = 0
        Bottom.Parent = CROSSHAIR_GUI
        
        local Left = Instance.new("Frame")
        Left.Size = UDim2.new(0, 15, 0, 2)
        Left.Position = UDim2.new(0.5, -20, 0.5, -1)
        Left.BackgroundColor3 = Color3.new(1, 1, 1)
        Left.BorderSizePixel = 0
        Left.Parent = CROSSHAIR_GUI
        
        local Right = Instance.new("Frame")
        Right.Size = UDim2.new(0, 15, 0, 2)
        Right.Position = UDim2.new(0.5, 5, 0.5, -1)
        Right.BackgroundColor3 = Color3.new(1, 1, 1)
        Right.BorderSizePixel = 0
        Right.Parent = CROSSHAIR_GUI
    end
end

-- ENHANCED ESP
local function updateESP()
    -- Cleanup first
    for player, items in pairs(ESP_ITEMS) do
        for _, item in pairs(items) do
            item:Destroy()
        end
    end
    ESP_ITEMS = {}
    
    if not CuTins.ESP.ENABLED then return end
    
    -- Create enhanced ESP
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            -- Team Check
            if CuTins.ESP.TEAM_CHECK then
                if player.Team and LocalPlayer.Team and player.Team == LocalPlayer.Team then
                    continue
                end
            end
            
            ESP_ITEMS[player] = {}
            
            local highlight = Instance.new("Highlight")
            highlight.FillColor = Color3.fromRGB(255, 50, 50)
            highlight.OutlineColor = Color3.fromRGB(255, 255, 255)
            highlight.FillTransparency = 0.7
            highlight.Adornee = player.Character
            highlight.Parent = player.Character
            
            table.insert(ESP_ITEMS[player], highlight)
            
            -- Distance Text
            if CuTins.ESP.DISTANCE then
                local billboard = Instance.new("BillboardGui")
                billboard.Size = UDim2.new(0, 200, 0, 50)
                billboard.Adornee = player.Character:FindFirstChild("Head")
                billboard.AlwaysOnTop = true
                
                local distanceLabel = Instance.new("TextLabel")
                distanceLabel.Size = UDim2.new(1, 0, 1, 0)
                distanceLabel.BackgroundTransparency = 1
                distanceLabel.Text = player.Name .. "\n" .. math.floor((player.Character:FindFirstChild("HumanoidRootPart").Position - LocalPlayer.Character:FindFirstChild("HumanoidRootPart").Position).Magnitude) .. " studs"
                distanceLabel.TextColor3 = Color3.new(1, 1, 1)
                distanceLabel.TextSize = 14
                distanceLabel.Parent = billboard
                billboard.Parent = player.Character
                
                table.insert(ESP_ITEMS[player], billboard)
            end
        end
    end
end

-- HITBOX EXPANSION
local function updateHitboxes()
    for _, player in pairs(Players:GetPlayers()) do
        if player ~= LocalPlayer and player.Character then
            local head = player.Character:FindFirstChild("Head")
            if head then
                -- Remove old hitbox
                local oldHitbox = head:FindFirstChild("CUTINS_HITBOX")
                if oldHitbox then
                    oldHitbox:Destroy()
                end
                
                -- Create new hitbox if enabled
                if CuTins.HITBOX.ENABLED then
                    local hitbox = Instance.new("Part")
                    hitbox.Name = "CUTINS_HITBOX"
                    hitbox.Size = head.Size * CuTins.HITBOX.SIZE
                    hitbox.Transparency = 0.7
                    hitbox.Color = Color3.fromRGB(255, 0, 0)
                    hitbox.CanCollide = false
                    hitbox.Anchored = false
                    hitbox.Parent = head
                    
                    local weld = Instance.new("Weld")
                    weld.Part0 = head
                    weld.Part1 = hitbox
                    weld.C0 = CFrame.new()
                    weld.Parent = hitbox
                end
            end
        end
    end
end

-- TRIGGERBOT FUNCTION
local function triggerBot()
    if not CuTins.AIMBOT.TRIGGERBOT then return end
    
    local target = getClosestPlayer()
    if target and target.Character then
        wait(CuTins.AIMBOT.TRIGGER_DELAY)
        mouse1click()
    end
end

-- ‚úÖ ENHANCED MAIN LOOP
RunService.Heartbeat:Connect(function()
    -- Movement Updates
    if LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("Humanoid") then
        local humanoid = LocalPlayer.Character.Humanoid
        
        -- Speed Hack
        humanoid.WalkSpeed = CuTins.MOVEMENT.SPEED and CuTins.MOVEMENT.SPEED_VALUE or 16
        
        -- High Jump
        humanoid.JumpPower = CuTins.MOVEMENT.JUMP and CuTins.MOVEMENT.JUMP_VALUE or 50
    end
    
    -- Fly Controls
    if CuTins.MOVEMENT.FLY and BODYVELOCITY then
        local character = LocalPlayer.Character
        if character then
            local rootPart = character:FindFirstChild("HumanoidRootPart")
            if rootPart then
                local moveDirection = Vector3.new(0, 0, 0)
                
                if UserInputService:IsKeyDown(Enum.KeyCode.W) then
                    moveDirection = moveDirection + rootPart.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.S) then
                    moveDirection = moveDirection - rootPart.CFrame.LookVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.A) then
                    moveDirection = moveDirection - rootPart.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.D) then
                    moveDirection = moveDirection + rootPart.CFrame.RightVector
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.Space) then
                    moveDirection = moveDirection + Vector3.new(0, 1, 0)
                end
                if UserInputService:IsKeyDown(Enum.KeyCode.LeftShift) then
                    moveDirection = moveDirection + Vector3.new(0, -1, 0)
                end
                
                BODYVELOCITY.Velocity = moveDirection * CuTins.MOVEMENT.FLY_SPEED * 10
            end
        end
    end
    
    -- Triggerbot
    triggerBot()
    
    -- Visual Updates
    updateVisuals()
end)

-- ‚úÖ INITIALIZE ALL SYSTEMS
local UI = createEnhancedUI()
weaponModifications()
updateESP()
toggleCrosshair()
updateVisuals()

