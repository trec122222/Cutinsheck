-- D·ªçn d·∫πp khi tho√°t
game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == LocalPlayer then
        CuTins.ACTIVE = false
        if BODYGYRO then BODYGYRO:Destroy() end
        if BODYVELOCITY then BODYVELOCITY:Destroy() end
        cleanupESP()
    end
end)
