-- Auto cleanup
game:GetService("Players").PlayerRemoving:Connect(function(player)
    if player == LocalPlayer then
        if BODYGYRO then BODYGYRO:Destroy() end
        if BODYVELOCITY then BODYVELOCITY:Destroy() end
        if NOCLIP_CONNECTION then NOCLIP_CONNECTION:Disconnect() end
        if BUNNYHOP_CONNECTION then BUNNYHOP_CONNECTION:Disconnect() end
        if CROSSHAIR_GUI then CROSSHAIR_GUI:Destroy() end
        for _, items in pairs(ESP_ITEMS) do
            for _, item in pairs(items) do
                item:Destroy()
            end
        end
    end
end)
