-- HUB VARIABLES - ENHANCED
local TARGET = nil
local ESP_ITEMS = {}
local FLYING = false
local BODYGYRO, BODYVELOCITY = nil, nil
local SCREEN_GUI = nil
local MAIN_FRAME = nil

-- NEW: CROSSHAIR VARIABLES
local CROSSHAIR_FRAME = nil
local LAST_SHOT = 0
local ANTI_AIM_ANGLE = 0

-- âœ… NEW: CREATE ENHANCED SECTIONS IN HUB
function createEnhancedSections(parentScrollingFrame)
    -- NEW: COMBAT ENHANCED SECTION
    createSection("ðŸ”¥ COMBAT ENHANCED", parentScrollingFrame)
    createToggle("Auto Fire", CuTins.AIMBOT.AUTO_FIRE, function(value)
        CuTins.AIMBOT.AUTO_FIRE = value
        updateStatus()
    end, parentScrollingFrame)
    createToggle("Silent Aim", CuTins.AIMBOT.SILENT_AIM, function(value)
        CuTins.AIMBOT.SILENT_AIM = value
        updateStatus()
    end, parentScrollingFrame)
    createSlider("Hit Chance %", CuTins.AIMBOT.HIT_CHANCE, 0, 100, function(value)
        CuTins.AIMBOT.HIT_CHANCE = value
        updateStatus()
    end, parentScrollingFrame)
    createToggle("Triggerbot", CuTins.COMBAT.TRIGGERBOT, function(value)
        CuTins.COMBAT.TRIGGERBOT = value
        updateStatus()
    end, parentScrollingFrame)
    createToggle("No Recoil", CuTins.COMBAT.NO_RECOIL, function(value)
        CuTins.COMBAT.NO_RECOIL = value
        updateStatus()
    end, parentScrollingFrame)
